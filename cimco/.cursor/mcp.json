{
	"mcpServers": {
		"storybook-cimco": {
			"command": "npx",
			"args": ["-y", "storybook-mcp@latest"],
			"env": {
				"STORYBOOK_URL": "https://YOUR_CIMCO_STORYBOOK_URL/index.json"
			}
		},
		"storybook-lifetime": {
			"command": "npx",
			"args": ["-y", "storybook-mcp@latest"],
			"env": {
				"STORYBOOK_URL": "https://YOUR_LIFETIME_STORYBOOK_URL/index.json"
			}
		}
	}
}

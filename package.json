{
  "name": "lifetime-fitness-maintenance-pwa",
  "version": "1.0.0",
  "description": "Lifetime Fitness Maintenance PWA - Comprehensive maintenance management for fitness facilities",
  "main": "index.js",
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 --cache --fix",
    "typecheck": "echo 'TypeScript checking not configured for this JavaScript project'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "agents:start": "node scripts/background-agents.js start",
    "agents:stop": "node scripts/background-agents.js stop",
    "agents:status": "node scripts/background-agents.js status",
    "agents:list": "node scripts/background-agents.js list",
    "ai:start": "node scripts/n8n-ai-agent.js start",
    "ai:stop": "node scripts/n8n-ai-agent.js stop",
    "ai:status": "node scripts/n8n-ai-agent.js status",
    "ai:trigger": "node scripts/n8n-ai-agent.js trigger",
    "ai:webhook": "node scripts/trigger-ai-agent.js",
    "dev:auto": "node scripts/dev-automation.js start",
    "dev:stop": "node scripts/dev-automation.js stop",
    "dev:status": "node scripts/dev-automation.js status",
    "quick:build": "node scripts/dev-automation.js build",
    "quick:test": "node scripts/dev-automation.js test",
    "quick:deploy": "node scripts/dev-automation.js deploy",
    "start": "node scripts/quick-start.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "gh:deploy": "gh pages deploy dist --add --push",
    "gh:create-issue": "gh issue create --title \"Feature Request\" --body \"New feature request\"",
    "gh:create-pr": "gh pr create --title \"Feature: \" --body \"Implements new feature\"",
    "start:hybrid": "node start-hybrid-system.js",
    "backend": "node start-fastapi.js",
    "backend:simple": "cd backend && python start-simple.py",
    "start:dynamic": "node scripts/dynamic-start.js",
    "start:ports": "node scripts/port-finder.js",
    "ports:status": "node scripts/port-status.js",
    "dev:dynamic": "node scripts/dynamic-start.js --frontend",
    "api:dynamic": "node scripts/dynamic-start.js --api",
    "backend:dynamic": "node scripts/dynamic-start.js --backend",
    "taskmaster": "node scripts/taskmaster-ai.js",
    "taskmaster:status": "node scripts/taskmaster-ai.js status",
    "taskmaster:next": "node scripts/taskmaster-ai.js next",
    "taskmaster:report": "node scripts/taskmaster-ai.js report"
  },
  "dependencies": {
    "@progress/kendo-react-buttons": "^11.3.1",
    "@progress/kendo-react-charts": "^11.3.1",
    "@progress/kendo-react-data-tools": "^11.3.1",
    "@progress/kendo-react-dateinputs": "^11.3.1",
    "@progress/kendo-react-dropdowns": "^11.3.1",
    "@progress/kendo-react-grid": "^11.3.1",
    "@progress/kendo-react-inputs": "^11.3.1",
    "@progress/kendo-react-layout": "^11.3.1",
    "@progress/kendo-react-notification": "^11.3.1",
    "@progress/kendo-react-scheduler": "^11.3.1",
    "@progress/kendo-theme-default": "^11.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.38.4",
    "chart.js": "^4.5.0",
    "date-fns": "^2.30.0",
    "fabric": "^6.7.1",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-camera-pro": "^1.4.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.8.1",
    "task-master-ai": "^0.22.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.1.0",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "pwa",
    "maintenance",
    "lifetime-fitness",
    "fitness-facility",
    "equipment-maintenance",
    "react"
  ],
  "author": "Lifetime Maintenance Team",
  "license": "MIT"
}

{
  "name": "lifetime-maintenance-pwa",
  "version": "1.0.0",
  "description": "Lifetime Fitness Maintenance PWA",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "agents:start": "node scripts/background-agents.js start",
    "agents:stop": "node scripts/background-agents.js stop",
    "agents:status": "node scripts/background-agents.js status",
    "agents:list": "node scripts/background-agents.js list",
    "dev:auto": "node scripts/dev-automation.js start",
    "dev:stop": "node scripts/dev-automation.js stop",
    "dev:status": "node scripts/dev-automation.js status",
    "quick:build": "node scripts/dev-automation.js build",
    "quick:test": "node scripts/dev-automation.js test",
    "quick:deploy": "node scripts/dev-automation.js deploy",
    "start": "node scripts/quick-start.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.38.4",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.1.0",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "pwa",
    "maintenance",
    "lifetime-fitness",
    "react"
  ],
  "author": "Lifetime Maintenance Team",
